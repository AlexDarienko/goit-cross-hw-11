{
  "name": "habitflow-perf-opt",
  "version": "3.0.1",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start -c",
    "web": "expo start --web",
    "doctor": "npm ls @react-native/codegen react-native || true",
    "postinstall": "bash scripts/postinstall-stub-debugging-overlay.sh",
    "build:web": "expo export --platform web",
    "analyze:web": "npm run build:web && source-map-explorer dist/_expo/static/js/*.js --html docs/bundle-report.html || echo 'Install source-map-explorer to run analysis'"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.2.3",
    "dayjs": "^1.11.12",
    "expo": "^54.0.10",
    "expo-font": "~12.0.9",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.29.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-expo": "^10.0.2",
    "source-map-explorer": "^2.5.3"
  },
  "overrides": {
    "@react-native/codegen": "0.74.0"
  },
  "resolutions": {
    "@react-native/codegen": "0.74.0"
  }
}
